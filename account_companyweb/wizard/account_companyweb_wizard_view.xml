<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="wizard_account_companyweb" model="ir.ui.view">
			<field name="name">account.companyweb.wizard.form</field>
			<field name="model">account.companyweb.wizard</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="wizard account companyweb">
					<group colspan="2">
						<field name="name" />
						<field name="vat_number" />
						<field name="street" />
						<field name="city" />
						<field name="zip" />
						<field name="startDate" />
						<field name="endDate" />
					</group>
					<group colspan="2">
						<field name="vat_liable"/>
						<field name="creditLimit" />
						<field name="equityCapital" />
						<field name="addedValue" />
						<field name="turnover" />
						<field name="result" />
					</group>
					<group colspan="4">
						<field name="warnings" required="False" />
						<field name="url" invisible="1" />
					</group>
					<group colspan="4">
						<field name="image" widget='image' readonly="1"/>
					</group>

					<group colspan="4">
						<button name="open_url_function" type="object"
							string="More informations about this company" help="More informations about this company on WebCOmpany.be"
							class="oe_link" />
					</group>

					<group colspan="2">
						<button name="act_destroy" special="cancel" string="Close"
							type="object" />
					</group>

					<group colspan="2">
						<button name="update_information" type="object"
							string="Update database information" />
					</group>
				</form>
			</field>
		</record>

	</data>
</openerp>